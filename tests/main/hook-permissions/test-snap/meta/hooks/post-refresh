#!/bin/sh

if snapctl is-connected upower-observe; then
    dbus-send --print-reply --system --dest=org.freedesktop.UPower /org/freedesktop/UPower org.freedesktop.UPower.EnumerateDevices 2>&1 > "$SNAP_COMMON/enumerate.txt"
else
    echo "upower-observe not connected" > "$SNAP_COMMON/enumerate.txt"
fi
